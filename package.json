{
  "name": "cryptovoxels",
  "version": "0.9.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "bundle": "browserify index.js -t babelify -o bundle.js",
    "test": "babel-tape-runner tests/*.js && standard"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babylonjs/core": "^4.0.3",
    "babel-tape-runner": "^3.0.0",
    "browserify": "^17.0.0",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "ndarray": "1.0.18",
    "node-fetch": "^2.6.1",
    "tinyify": "^2.5.1",
    "uuid": "3.3.2",
    "ws": "7.4.6"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "targets": {
                  "chrome": "80"
                }
              }
            ]
          ],
          "sourceType": "module"
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "babel-loader": "^8.2.2",
    "babelify": "^10.0.0",
    "standard": "12.0.1",
    "tape": "4.9.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0"
  },
  "standard": {
    "ignore": [
      "bundle*"
    ]
  }
}
