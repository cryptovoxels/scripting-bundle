{
  "compilerOptions": {
    "sourceMap": false,
    "lib": ["es2017", "dom"],
    "target": "es5",
    "typeRoots": ["./node_modules/*"],
    "types": ["node"],
    "experimentalDecorators": true,
    "strict": true,
    "stripInternal": true,
    "skipLibCheck":true,
    "downlevelIteration":true
  }
}
